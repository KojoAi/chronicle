local testSection = AimingTab:CreateSector("Game", "left")


testSection:AddButton("Server-Time Checker", function(IhateGayPeople)
     -- Get the LocalPlayer
local player = game.Players.LocalPlayer

-- Create a ScreenGui for the GUI
local gui = Instance.new("ScreenGui")
gui.Parent = player.PlayerGui

-- Create a TextLabel for displaying the server time
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(0, 200, 0, 50) -- Set the size of the TextLabel
textLabel.Position = UDim2.new(1, -260, 0, -20) -- Position it 40 pixels to the left and 20 pixels up from the top right corner
textLabel.BackgroundTransparency = 1 -- Make the background transparent
textLabel.TextStrokeTransparency = 0.5 -- Add a black stroke to the text (0 is fully opaque, 1 is fully transparent)
textLabel.TextStrokeColor3 = Color3.new(0, 0, 0) -- Set the stroke color to black
textLabel.TextSize = 16 -- Set the text size
textLabel.TextXAlignment = Enum.TextXAlignment.Right -- Align text to the right
textLabel.TextYAlignment = Enum.TextYAlignment.Top -- Align text to the top
textLabel.TextColor3 = Color3.new(1, 0, 0) -- Set text color to red
textLabel.Parent = gui

-- Function to update the TextLabel text with the server time
local function updateServerTime()
    local currentTime = os.time()
    local serverTime = os.date("%Y-%m-%d %H:%M:%S", currentTime)
    textLabel.Text = "Server Time: " .. serverTime
end

-- Update the server time every second
while true do
    updateServerTime()
    wait() -- Wait for 1 second before updating the time again
end

end)


