local testSection = AimingTab:CreateSector("Auto-Farm Orb", "Right")  --you can  change the section code, for example "testsection" can be changed to "FunnyCoolSection" etc.

local RunService = game:GetService("RunService")

local function teleportPlayerAndActivateProximityPrompt(targetPosition)
    local humanoidRootPart = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    if humanoidRootPart and humanoidRootPart:IsA("BasePart") then
        humanoidRootPart.CFrame = CFrame.new(targetPosition)
    end

    activateProximityPrompts() -- Activate the proximity prompt immediately after teleporting
end

local function activateProximityPrompts()
    for i = 1, 1500 do
        fireproximityprompt(workspace.Game_FX.CurseSpinOrb.CurseSpinOrb.Collect)
    end
end

local function toggleAutoFarm()
    local isAutoFarming = true

    while isAutoFarming do
        teleportPlayerAndActivateProximityPrompt(workspace.Game_FX.CurseSpinOrb.CurseSpinOrb.Position)
        wait(31) -- Wait for 31 seconds before teleporting and activating again
    end
end

-- Inside the toggle function
testSection:AddToggle("Auto-Farm CurseSpinOrb", false, function(isToggled)
    if isToggled then
        toggleAutoFarm()
    else
        -- Stop the auto-farming process (if needed)
    end
end)



